<%= render(partial:  'headers/header_employees', locals: { :tab=>'employees' }) %>
<h2><%= l(:label_employee_edit) %></h2>
<%= labelled_form_for @employee, html: {multipart: true, method: :put} do |f| %>
  <%= error_messages_for 'employee' %>
  <%= render partial:  'form', locals: {f: f} %>
  <%= submit_tag l(:button_save) %>
<% end %>
