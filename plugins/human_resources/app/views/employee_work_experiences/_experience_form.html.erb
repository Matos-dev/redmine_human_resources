<%= error_messages_for @work_experience %>

<div id="main_o" class="box tabular">
  <p>
    <%= f.text_field :previous_company_name, required: true %>
  </p>
  <p>
    <%= f.text_field :job_title, required: true %>
  </p>
  <p><%= f.date_field :from_date, size: 12, required: true %><%#= calendar_for('employee_work_experience_from_date') %> </p>
  <p><%= f.date_field :to_date, size: 12, required: true %><%#= calendar_for('employee_work_experience_to_date') %> </p>
  <p><%= f.text_area :description, cols: 60, rows: 10, class:  'wiki-edit' %>
</div>